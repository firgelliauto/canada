{% comment %}
This file is auto-generated by PageFly. The content can be overridden with saving. Please do not update directly in this file.
PLEASE REMOVE ONLY AFTER YOU UNINSTALLED THE PAGEFLY APP.
{% endcomment %}
<!doctype html>
<html class="no-js" lang="{{ shop.locale }}">
<head>
  {{ 'styles.css' | asset_url | stylesheet_tag }}
  {%- comment -%}smartseo{%- endcomment -%}
  {% case shop.address.country  %}
    {% when 'United States' %}
      {%- comment -%}smartseo_integration_begin{%- endcomment -%}
      {% include 'smartseo' %}
      {%- comment -%}smartseo_integration_end{%- endcomment -%}
      {%- comment smartseo_integration_begin_meta_data -%}<title>{{ page_title }}</title>{%- endcomment smartseo_integration_end_meta_data -%}
      {%- comment smartseo_integration_begin_meta_data -%}<meta name="description" content="{{ page_description }}">{%- endcomment smartseo_integration_end_meta_data -%}
    {% when 'Canada' %}
      {% include 'fa-seo' %}
      <title>{{ page_title }}</title>
      <meta name="description" content="{{ page_description }}">
    {% when 'Australia' %}
      {% include 'fa-seo' %}
      <title>{{ page_title }}</title>
      <meta name="description" content="{{ page_description }}">
  {% endcase %}

  <meta charset="utf-8">
  {% include 'script-header-per-location' %}
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <meta http-equiv="Expires" content="max-age=2628000" />
  <meta property="al:android:package" content="google-app-package">
  <meta property="al:android:app_name" content="Firgelli Automations app">

  <link rel="canonical" href="{{ canonical_url }}">
  {% include 'fa-hreflang-router' %}
  {% include 'script-header-favicon' %}
  {% include 'social-meta-tags' %}
  {% include 'script-g-optimize' %}
  
  {% include 'css-typography' %}
 

  {% if template == 'index' %}
    
  {% else %}
  {{ 'header-all.js' | asset_url | script_tag }}
  
  {% endif %}
  <script async src="{{ 'uikit.min.js' | asset_url }}"></script>
  <script async src="{{ 'uikit-icons.min.js' | asset_url }}"></script>
  {%- if template == 'index' and settings.let_snow -%}
    <script src="https://cdn.jsdelivr.net/particles.js/2.0.0/particles.min.js" async defer></script>
  {%- endif -%}

    <script src="{{ 'anime.min.js' | asset_url }}" defer></script>
  {%- if template == 'index' or template == 'cart' or template == 'article' -%}
  {%- endif -%}

  <script>
    document.documentElement.className = document.documentElement.className.replace("no-js","js");

    window.theme = {
      strings: {
        addToCart: {{ 'products.product.add_to_cart' | t | json }},
        soldOut: {{ 'products.product.sold_out' | t | json }},
        unavailable: {{ 'products.product.unavailable' | t | json }}
      },
      moneyFormat: {{ shop.money_format | json }}
    };
  </script>

  {% if template == 'page.calculator' %}
    {% include 'calculator-links' %}
  {% endif %}

  {{ content_for_header }}
  {%- render 'bold-common' -%}
 
{% include 'pagefly-header' %}
 </head> 
{% assign style_mod = '' %}
{%- if settings.warm_buttons == true -%}
  {% assign style_mod = 'fg-warm-buttons' %}
{%- endif -%}
<body id="{{ page_title | handle }}" class="template-{{ template.name | handle }} {{ style_mod }}" data-curr-format="{{shop.money_format}}" data-curr-code="{{shop.currency}}" ><div class="pagefly-container">{{ content_for_layout }}</div></body>
</html>
