{% comment %}
{%- if settings.favicon != blank -%}
  <link rel="shortcut icon" href="{{ settings.favicon | img_url: '16x16' }}" type="image/png" sizes="16x16">
  <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png" sizes="32x32">
  <link rel="shortcut icon" href="{{ settings.favicon | img_url: '192x192' }}" type="image/png" sizes="192x192">
  <link rel="apple-touch-icon-precomposed" href="{{ settings.favicon | img_url: '152x152' }}" /> 
{%- endif -%}{% endcomment %}

<link rel="apple-touch-icon" sizes="180x180" href="{{ "apple-touch-icon.png" | asset_url }}">
<link rel="icon" type="image/png" sizes="32x32" href="{{ "favicon-32x32.png" | asset_url }}">
<link rel="icon" type="image/png" sizes="16x16" href="{{ "favicon-16x16.png" | asset_url }}">
<link rel="manifest" href="{{ "site.webmanifest" | asset_url }}">
<link rel="mask-icon" href="{{ "safari-pinned-tab.svg" | asset_url }}" color="#5bbad5">
<meta name="msapplication-TileColor" content="#da532c">
<meta name="theme-color" content="#ffffff">
